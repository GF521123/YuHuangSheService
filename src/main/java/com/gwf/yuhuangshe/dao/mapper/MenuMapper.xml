<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gwf.yuhuangshe.dao.MenuMapper">

    <resultMap id="MenuMap" type="com.gwf.yuhuangshe.entity.Menu">
        <result column="MId" jdbcType="INTEGER" property="MId" />
        <result column="MTitle" jdbcType="VARCHAR" property="MTitle" />
        <result column="MValue" jdbcType="VARCHAR" property="MValue" />
        <result column="MSort" jdbcType="INTEGER" property="MSort" />
        <result column="MUrl" jdbcType="VARCHAR" property="MUrl" />
        <result column="MOther" jdbcType="VARCHAR" property="MOther" />
    </resultMap>


    <select id="selMenu" resultMap="MenuMap" resultType="com.gwf.yuhuangshe.entity.Menu">
        select MId,MTitle,MValue,MSort,MUrl,MOther from u_menu
    </select>

</mapper>


